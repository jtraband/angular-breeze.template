{
  "name": "sc-client",
  "version": "1.0.44",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "tsc": "tsc",
    "start": "ng serve -scd=false",
    "start-dist": "node tools/server.js",
    "build": "node --max_old_space_size=8192 ./node_modules/@angular/cli/bin/ng build --build-optimizer=false -vc -ec",
    "build-dist": "npm run build -- --target=production --environment=dev --stats-json",
    "build-prod": "npm run build -- --target=production",
    "build-sctest": "npm run build -- --env=sctest --target=production --base-href=/Insight/",
    "build-azure": "npm run build -- --env=azure --target=production --base-href=/Insight/",
    "build-azvirtdir": "npm run build -- --env=azvirtdir --target=production --base-href=/Insight/",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "bundle-report": "webpack-bundle-analyzer dist/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^5.1.3",
    "@angular/common": "^5.1.3",
    "@angular/compiler": "^5.1.3",
    "@angular/core": "^5.1.3",
    "@angular/forms": "^5.1.3",
    "@angular/platform-browser": "^5.1.3",
    "@angular/platform-browser-dynamic": "^5.1.3",
    "@angular/router": "^5.1.3",
    "bootstrap": "^4.0.0-beta.3",
    "breeze-bridge2-angular": "^1.0.0-beta.0",
    "breeze-client": "^1.6.3",
    "core-js": "^2.5.3",
    "devextreme": "^17.2.4",
    "devextreme-angular": "^17.2.4",
    "font-awesome": "^4.7.0",
    "jquery": "^3.2.1",
    "lodash": "^4.17.4",
    "moment-mini": "^2.18.1",
    "@ng-idle/core": "^2.0.0-beta.13",
    "@ng-idle/keepalive": "^2.0.0-beta.13",
    "ngx-bootstrap": "1.7.1",
    "popper.js": "^1.12.9",
    "rxjs": "^5.5.6",
    "zone.js": "^0.8.19"
  },
  "devDependencies": {
    "@angular/cli": "^1.6.3",
    "@angular/compiler-cli": "^5.1.3",
    "@angular/language-service": "^5.1.3",
    "@types/file-saver": "0.0.1",
